{
  "schemaVersion": 38,
  "title": "RF Presence Intelligence v2",
  "panels": [
    {
      "type": "timeseries",
      "title": "Unknown Devices",
      "targets": [
        {
          "expr": "unknown_devices_nearby"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Ring Correlations",
      "targets": [
        {
          "expr": "increase(correlations_total[1m])"
        }
      ]
    },
    {
      "type": "heatmap",
      "title": "RSSI Heat",
      "targets": [
        {
          "expr": "correlation_score_bucket"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Alerts Firing",
      "targets": [
        {
          "expr": "ALERTS{alertstate='firing'}"
        }
      ]
    }
  ]
}